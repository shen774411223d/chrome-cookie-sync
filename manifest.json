{
  "name": "CookieSync",
  "version": "1.0",
  "description": "Sync Cookie from other domains",
  "permissions": ["cookies", "<all_urls>", "tabs", "storage"],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
